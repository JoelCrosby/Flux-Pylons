{
  "multipart": [
    {
      "apply": {
        "model": "fluxpylons:block/adv_pipe_center"
      }
    },
    {
      "when": {
        "north": "connected"
      },
      "apply": {
        "model": "fluxpylons:block/adv_pipe_connected"
      }
    },
    {
      "when": {
        "south": "connected"
      },
      "apply": {
        "model": "fluxpylons:block/adv_pipe_connected",
        "y": 180
      }
    },
    {
      "when": {
        "east": "connected"
      },
      "apply": {
        "model": "fluxpylons:block/adv_pipe_connected",
        "y": 90
      }
    },
    {
      "when": {
        "west": "connected"
      },
      "apply": {
        "model": "fluxpylons:block/adv_pipe_connected",
        "y": 270
      }
    },
    {
      "when": {
        "down": "connected"
      },
      "apply": {
        "model": "fluxpylons:block/adv_pipe_connected",
        "x": 90
      }
    },
    {
      "when": {
        "up": "connected"
      },
      "apply": {
        "model": "fluxpylons:block/adv_pipe_connected",
        "x": 270
      }
    },
    {
      "when": {
        "north": "end"
      },
      "apply": {
        "model": "fluxpylons:block/adv_pipe_end"
      }
    },
    {
      "when": {
        "south": "end"
      },
      "apply": {
        "model": "fluxpylons:block/adv_pipe_end",
        "y": 180
      }
    },
    {
      "when": {
        "east": "end"
      },
      "apply": {
        "model": "fluxpylons:block/adv_pipe_end",
        "y": 90
      }
    },
    {
      "when": {
        "west": "end"
      },
      "apply": {
        "model": "fluxpylons:block/adv_pipe_end",
        "y": 270
      }
    },
    {
      "when": {
        "down": "end"
      },
      "apply": {
        "model": "fluxpylons:block/adv_pipe_end",
        "x": 90
      }
    },
    {
      "when": {
        "up": "end"
      },
      "apply": {
        "model": "fluxpylons:block/adv_pipe_end",
        "x": 270
      }
    }
  ]
}
